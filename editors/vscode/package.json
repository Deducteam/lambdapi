{
  "name": "vscode-lp",
  "version": "0.0.0",
  "publisher": "MINES ParisTech / INRIA",
  "engines": {
    "vscode": "^1.37.0"
  },
  "enableProposedApi": true,
  "activationEvents": [
    "*",
    "onCommand:getGoals.start",
    "onLanguage:lp"
  ],
  "main": "out/client.js",
  "contributes": {
    "languages": [
      {
        "id": "lp",
        "aliases": [
          "Dedukti",
          "Lambdapi"
        ],
        "extensions": [
          ".lp"
        ],
        "configuration": "./lp.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lp",
        "scopeName": "source.lp",
        "path": "./syntaxes/lp.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "getGoals.start",
        "title": "goals"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.14.15",
    "@types/vscode": "^1.37",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "vscode-languageclient": "5.2.1"
  }
}
