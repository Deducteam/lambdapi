{
  "name": "vscode-lp",
  "version": "0.0.0",
  "publisher": "MINES ParisTech / INRIA",
  "engines": {
    "vscode": "^1.35.0"
  },
  "activationEvents": [
    "onLanguage:lp"
  ],
  "main": ".out/src/protocol.js",
  "contributes": {
    "languages": [
      {
        "id": "lp",
        "aliases": [
          "Dedukti",
          "Lambdapi"
        ],
        "extensions": [
          ".lp"
        ],
        "configuration": "./lp.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lp",
        "scopeName": "source.lp",
        "path": "./syntaxes/lp.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "vscode-lp.restart",
        "title": "vscode start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "vscode-languageclient": "5.2.1"
  }
}
